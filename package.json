    "postcss": "^8.4.16",
    "standard": "17.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint . --fix",
      "npx prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
