    "framer-motion": "6.3.15",
    "google-map-react": "^2.1.10",
    "husky": "^8.0.1",
    "javascript-time-ago": "^2.5.7",
    "jwt-decode": "^3.1.2",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-streetview": "0.0.9",
    "react-select": "5.3.2",
    "react-spinners": "0.12.0",
    "react-streetview": "^1.1.1",
    "swr": "1.3.0",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@types/canvas-confetti": "1.4.2",
    "@types/google-map-react": "^2.1.7",
